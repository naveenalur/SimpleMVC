@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<div id="person-list"></div>
@*<a class="btn btn-default" id="CreateChart"><i class="fa fa-bar-chart"></i> Call Chart</a>*@


<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

<script type="text/javascript" src="js/script.js"></script>

<script type="text/javascript">

    $('#CreateChart').click(function () {
        debugger;
        $.ajax({
            url: "@Url.Action("CreateGraph", "Home")",
            type: "Get",

            cache: false,
            async: true,
            success: function (data) {
              // var personData = jQuery.parseJSON(data);

                $.each(data, function() {
                    var html = '<div class="person-list-item"><div>' + this.Name + '</div><div>' + this.Gender+ '</div></div>';
                    $('#person-list').append(html);
                });
                debugger;
                alert(data);
            }
        });
    });
</script>